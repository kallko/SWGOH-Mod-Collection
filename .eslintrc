{
	"extends": [
	],
	"rules": {
		"no-console": 2,
		"no-underscore-dangle": 0,
		"strict": 0,
		"no-use-before-define": 0,
		"eqeqeq": 2,
		"no-dupe-keys": 2,

		"no-unused-vars": [
			2,
			{
				"vars": "all",
				"args": "none"
			}
		],
		"no-shadow": 0,
		"consistent-return": 0,
		"no-multi-spaces": 0,
		"no-alert": 0,
		"default-case": 1,
		"no-case-declarations": 1,

		// stylistic
		"brace-style": [
			2,
			"1tbs",
			{
				"allowSingleLine": true
			}
		],
		"comma-spacing": 2,
		"camelcase": [
			2,
			{
				"properties": "never"
			}
		],
		"eol-last": 2,
		"consistent-this": [2, "_this"],
		"linebreak-style": [2, "unix"],
		"key-spacing": 0,
		"new-cap": 0,
		"jsx-quotes": "off",
		"new-parens": 2,
		"no-array-constructor": 2,
		"no-lonely-if": 2,
		"no-multiple-empty-lines": 2,
		"no-nested-ternary": 2,
		"no-mixed-spaces-and-tabs": [2, "smart-tabs"],
		"no-new-object": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 2,
		"object-curly-spacing": 0,
		"one-var": [
			2,
			{
				"initialized": "never"
			}
		],
		"quote-props": [
			2,
			"as-needed",
			{ "keywords": false, "unnecessary": false }
		],
		"semi-spacing": 2,
		"semi": [2, "always"],
		"keyword-spacing": 2,
		"space-before-blocks": 2,
		"space-infix-ops": 2,
		"space-unary-ops": 2,
		"no-useless-escape": 0,
		"no-restricted-globals": ["error", "event", "document"]
	},
	"env": {
		"node": true,
		"mocha": true,
		"es6": true
	},
	"plugins": ["react"],
	"parserOptions": {
		"ecmaVersion": 2017,
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		}
	},
	"parser": "babel-eslint"
}
